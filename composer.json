{
  "name": "exinone/mixin-sdk-php",
  "description": "Mixin-Network SDK for PHP, modify from ExinOne/laravel-mixin-sdk",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "ExinOne\\MixinSDK\\": "src/"
    },
    "files": [
      "helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ExinOne\\MixinSDK\\Tests\\": "tests/"
    }
  },
  "require": {
    "ext-json": "*",
    "ext-openssl": "*",
    "ext-zlib": "*",
    "ext-sodium": "*",
    "ext-bcmath": "*",
    "php": "^7.2 || ^8.0",
    "firebase/php-jwt": "^5.0",
    "guzzlehttp/guzzle": "^6.3 || ^7.0",
    "ramsey/uuid": "^3.8 || ^4",
    "phpseclib/phpseclib": "^2.0",
    "wrench/wrench": "^2.0",
    "web-token/jwt-easy": "^2.2",
    "web-token/jwt-key-mgmt": "^2.2",
    "web-token/jwt-signature-algorithm-eddsa": "^2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0 || ^8.0",
    "symfony/var-dumper": "^4.2"
  }
}
